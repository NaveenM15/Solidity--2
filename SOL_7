 pragma solidity ^0.8.0;
 contract Naveen{
    string public name="Nawerwwwwwwwwwwveen";
    function test()public view returns(string memory){
        return name;
    }
 }

 contract NextContract is Naveen{
    function NextTester() public view returns(string memory){
        return name;
    }

 }

